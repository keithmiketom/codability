<h1>Listing details</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Age</th>
    <th>Email</th>
    <th>Lengthteach</th>
    <th>Profession</th>
    <th>Ictlevel</th>
    <th>Research</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @details.each do |detail| %>
  <tr>
    <td><%= detail.name %></td>
    <td><%= detail.age %></td>
    <td><%= detail.email %></td>
    <td><%= detail.lengthteach %></td>
    <td><%= detail.profession %></td>
    <td><%= detail.ictlevel %></td>
    <td><%= detail.research %></td>
    <td><%= link_to 'Show', detail %></td>
    <td><%= link_to 'Edit', edit_detail_path(detail) %></td>
    <td><%= link_to 'Destroy', detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

